generator client {
    provider = "prisma-client-js"
}

//from vercl postgres docs
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Event {
  id            String   @id
  object        String
  actor_id      String
  actor_name    String
  group         String
  action        Json
  target_id     String
  target_name   String
  location      String
  occurred_at   DateTime @default(now())
  metadata      Json
}
